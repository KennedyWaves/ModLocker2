<Script>function switchON()
	Shell.Execute("Adm_Locker.exe", "open", "/sampunlock", "", SW_SHOWNORMAL, true);
	Button.SetEnabled("cmd_addcomsa", true);
	Button.SetEnabled("cmd_ver", true);
	Button.SetEnabled("cmd_addincomsa", true);
	Button.SetEnabled("cmd_apli", true);
	Button.SetEnabled("cmd_helpgta", true);
	Button.SetEnabled("cmd_instalar", true);
	Button.SetEnabled("cmd_jogaragora", true);
	Button.SetEnabled("cmd_limpacomsa", true);
	Button.SetEnabled("cmd_limpaincomsa", true);
	Button.SetEnabled("cmd_makeatalho", true);
	Button.SetEnabled("cmd_removecomsa", true);
	Button.SetEnabled("cmd_removeincomsa", true);
	Button.SetEnabled("cmd_remover", true);
	ListBox.SetEnabled("ltb_comsa", true);
	ListBox.SetEnabled("ltb_incomsa", true);
	RadioButton.SetEnabled("rdb_askallsamp", true);
	RadioButton.SetEnabled("rdb_askofflistsamp", true);
	RadioButton.SetEnabled("rdb_offallsamp", true);
	RadioButton.SetEnabled("rdb_onallsamp", true);
	exportdir = sampdir;
	loadnplugin();
end
	
function switchOFF()
	Button.SetEnabled("cmd_ver", false);
	Button.SetEnabled("cmd_addcomsa", false);
	Button.SetEnabled("cmd_addincomsa", false);
	Button.SetEnabled("cmd_apli", true);
	Button.SetEnabled("cmd_helpgta", true);
	Button.SetEnabled("cmd_instalar", false);
	Button.SetEnabled("cmd_jogaragora", false);
	Button.SetEnabled("cmd_limpacomsa", false);
	Button.SetEnabled("cmd_limpaincomsa", false);
	Button.SetEnabled("cmd_makeatalho", false);
	Button.SetEnabled("cmd_removecomsa", false);
	Button.SetEnabled("cmd_removeincomsa", false);
	Button.SetEnabled("cmd_remover", false);
	ListBox.SetEnabled("ltb_comsa", false);
	ListBox.SetEnabled("ltb_incomsa", false);
	RadioButton.SetEnabled("rdb_askallsamp", false);
	RadioButton.SetEnabled("rdb_askofflistsamp", false);
	RadioButton.SetEnabled("rdb_offallsamp", false);
	RadioButton.SetEnabled("rdb_onallsamp", false);
	Paragraph.SetText("prg_plugins", "Plug-ins ASI instalados: 0\r\nPlugins CLEO instalados: 0\r\nTotal de plug-ins instalados: 0");
end

function carregaSAMP()
	if sampinit == 01 then
		RadioButton.SetChecked("rdb_offallsamp", true);
	end
	if sampinit == 02 then
		RadioButton.SetChecked("rdb_onallsamp", true);
	end
	if sampinit == 03 then
		RadioButton.SetChecked("rdb_askofflistsamp", true);
	end
	if sampinit == 04 then
		RadioButton.SetChecked("rdb_askallsamp", true);
	end
	Input.SetText("txt_sampdir", sampdir);
	--listcomsa
	lidoincomsamp = TextFile.ReadToTable("Mod_Locker\\SAMP_listcom.db");
	if lidoincomsamp[4] ~= "" then
		Table.Insert(lidoincomsamp, 4, "");
	end
	totallidosamp = Table.Count(lidoincomsamp);
	numero = 5;
	if totallidosamp &gt; 4 then
		for x = 5,totallidosamp do
			ListBox.AddItem("ltb_comsa", lidoincomsamp[x], "");
		end
	end
	--fim
	--listincomsa
	lidocomsamp = TextFile.ReadToTable("Mod_Locker\\SAMP_listincom.db");
	if lidocomsamp[4] ~= "" then
		Table.Insert(lidocomsamp, 4, "");
	end
	local totallidosamp = Table.Count(lidocomsamp);
	numero = 5;
	if lidocomsamp[4] ~= "" then
		Table.Insert(lidocomsamp, 4, "");
	end
	if totallidosamp &gt; 4 then
		for x = 5,totallidosamp do
			ListBox.AddItem("ltb_incomsa", lidocomsamp[x], "");
		end
	end
	--fim
	inittimesamp = inittimesamp + 1;
end

Load_dados();
ChangeWindowType();
transparency = 255;
while transparency &gt;= 245 do
	transparency = transparency - 1
	SetWindowTransparency(transparency);
	Application.Sleep(40);
end
exportdir = sampdir;
if inittimesamp == 0 then
	if sampdir ~= "" then
		switchON();
	end
	carregaSAMP();
end

Image.Load("cmd_samp", "Mod_Locker\\imagens\\samphigh.png");
Image.Load("cmd_gtasa", "Mod_Locker\\imagens\\gtanormal.png");
Image.Load("img_main", "Mod_Locker\\imagens\\wml_normal.png");
Image.Load("cmd_mtasa", "Mod_Locker\\imagens\\mtasanormal.png");


if sampdir == "" then
	switchOFF();
end
loadnplugin();
</Script>
