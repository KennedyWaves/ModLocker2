	textincommtasa = ListBox.GetItemText("ltb_incomsa", x);
	Table.Insert(gravaincommtasa, Table.Count(gravaincommtasa)+1, textincommtasa);
end

if gravaincommtasa[4] ~= "" then
	Table.Insert(gravaincommtasa, 4, "");
end

totalgravamtasa = Table.Count(gravaincommtasa);
compatmtasa = TextFile.ReadToString("Mod_Locker\\mtasa_padcom.db");
