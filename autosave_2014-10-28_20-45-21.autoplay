	if gravaincomsamp[x] then
		esta = String.Find(compatsamp, gravaincomsamp[x], 1, false);
		if esta ~= -1 then
			erro = erro + 1;
		end
	end
end
if totalgravasamp &lt; 5 then
	erro = 0;
end
if erro ~= 0 then
	img_msg = 3;
	txt_msg = "Um ou mais plug-ins desta lista estão incluídos na lista padrão de plug-ins compatíveis. Deseja salvar assim mesmo?";
