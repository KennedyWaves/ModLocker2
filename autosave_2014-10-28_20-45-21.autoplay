<Script>function ChangeWindowType()
	local myWndHnd = Application.GetWndHandle();
	local myDllArgs =  ",-20,524288";
	
	res = DLL.CallFunction(_SystemFolder .. "\\user32.dll", "SetWindowLongA", myWndHnd .. myDllArgs, DLL_RETURN_TYPE_LONG, DLL_CALL_STDCALL);
	error = Application.GetLastError();
	if (error ~= 0) then
		Dialog.Message("ChangeWindowType() failed", _tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
	end
end

function SetWindowTransparency(transparency)
	local myWndHnd = Application.GetWndHandle();
	local myDllArgs =  ",0,"..transparency..",2";

	res = DLL.CallFunction(_SystemFolder .. "\\user32.dll", "SetLayeredWindowAttributes", myWndHnd .. myDllArgs, DLL_RETURN_TYPE_LONG, DLL_CALL_STDCALL);
	error = Application.GetLastError();
	if (error ~= 0) then
		Dialog.Message("SetWindowTransparency() failed", _tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
	end
end

