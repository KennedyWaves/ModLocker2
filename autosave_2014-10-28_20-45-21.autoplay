function menu_select()
	tblMenu = {};
	tblMenu[1] ={};
	tblMenu[1].Text = "Marcar todos";
	tblMenu[1].ID = 100;
	tblMenu[1].IconID = 0;
	tblMenu[1].Checked = false;
	tblMenu[1].Enabled = true;
	tblMenu[2] ={};
	tblMenu[2].Text = "Desmarcar todos";
	tblMenu[2].ID = 200;
	tblMenu[2].IconID = 0;
	tblMenu[2].Checked = false;
	tblMenu[2].Enabled = true;
	mousePos = System.GetMousePosition(true, nil);
	total_list = ListBox.GetCount(setlistbox);
	POP = Application.ShowPopupMenu((mousePos.X)-250, mousePos.Y, tblMenu, TPM_LEFTALIGN, TPM_TOPALIGN, true, true);
	if(POP == 100)then
		for x=1, total_list do
			ListBox.SetItemCheck(setlistbox, x, BST_CHECKED);
		end
	end
	 
	if(POP == 200)then
		for x=1, total_list do
			ListBox.SetItemCheck(setlistbox, x, BST_UNCHECKED);
		end
	end
end

function loadnplugin()
	local cleo = File.Find(""..exportdir.."\\cleo", "*.cs", false, false, nil, nil);
	local asi = File.Find(exportdir, "*.asi", false, false, nil, nil);
	local totalcleo = 0;
	local totalasi = 0;
	local totalmods = 0;
	if (cleo) then
		totalcleo = Table.Count(cleo);
		totalmods = totalmods + totalcleo;
	end
	if (asi) then
		totalasi = Table.Count(asi);
		totalmods = totalmods + totalasi;
	end
	Paragraph.SetText("prg_plugins", "Plug-ins ASI instalados: "..totalasi.."\r\nPlugins CLEO instalados: "..totalcleo.."\r\nTotal de plug-ins instalados: "..totalmods);
end</Script>
