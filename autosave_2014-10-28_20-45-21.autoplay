for x=1,totalincomsamp do
	textincomsamp = ListBox.GetItemText("ltb_incomsa", x);
	Table.Insert(gravaincomsamp, Table.Count(gravaincomsamp)+1, textincomsamp);
end

if gravaincomsamp[4] ~= "" then
	Table.Insert(gravaincomsamp, 4, "");
end

totalgravasamp = Table.Count(gravaincomsamp);
compatsamp = TextFile.ReadToString("Mod_Locker\\SAMP_padcom.db");
erro = 0;
