<Args/>
<Script>delete = ListBox.GetChecked("ltb_plugins", BST_CHECKED);

if delete then
	local listmark = delete[1]
	totaldelete = Table.Count(delete);
	Application.SaveValue("Mod_Locker", "delete_number", totaldelete);		
	for x,y in pairs (delete) do
		deletetext = ListBox.GetItemText("ltb_plugins", delete[x]);
		if (String.Find(deletetext, ".cs", 1, false) ~= -1) then
			moddir = ""..exportdir.."\\cleo";
		end
		if (String.Find(deletetext, ".asi", 1, false) ~= -1) then
			moddir = exportdir;
		end
		Application.SaveValue("Mod_Locker", "deletemod_"..x, moddir.."\\"..deletetext);
	end
	Shell.Execute("Adm_Locker.exe", "open", "/removemod", "", SW_SHOWNORMAL, true);
	ListBox.DeleteItem("ltb_plugins", -1);
	if checktodos == true then
		XaddpluginAll();
	else
		Xaddplugin();
	end
	ListBox.SelectItem("ltb_plugins", listmark);
else
	img_msg = 2;
	txt_msg = "Por favor, marque um ou mais Plug-ins!";
	enablecmd1 = false;
	enablecmd2 = true;
	one_cmdtext = "";
	two_cmdtext = "OK";
	DialogEx.Show("Any_msg", true, nil, nil);
end
</Script>
