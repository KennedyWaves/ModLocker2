function apl()
	loadcfg = TextFile.ReadToTable("Mod_Locker\\MOD_LOCKER.cfg");
	if page == "gta" then
		saoffall = RadioButton.GetChecked("rdb_offallsa");
		saonall = RadioButton.GetChecked("rdb_onallsa");
		saaskofflist = RadioButton.GetChecked("rdb_askofflistsa");
		saaskall = RadioButton.GetChecked("rdb_askallsa");
		if (saoffall) then
			savecfgsa = 01;
		end
		if (saonall) then
			savecfgsa = 02;
		end
		if (saaskofflist) then
			savecfgsa = 03;
		end
		if (saaskall) then
			savecfgsa = 04;
		end
