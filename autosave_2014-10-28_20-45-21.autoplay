<Script>function switchON()
	Shell.Execute("Adm_Locker.exe", "open", "/saunlock", "", SW_SHOWNORMAL, true);
	Button.SetEnabled("cmd_addcomsa", true);
	Button.SetEnabled("cmd_addincomsa", true);
	Button.SetEnabled("cmd_apli", true);
	Button.SetEnabled("cmd_helpgta", true);
	Button.SetEnabled("cmd_instalar", true);
	Button.SetEnabled("cmd_jogaragora", true);
	Button.SetEnabled("cmd_limpacomsa", true);
	Button.SetEnabled("cmd_limpaincomsa", true);
	Button.SetEnabled("cmd_makeatalho", true);
	Button.SetEnabled("cmd_ver", true);
	Button.SetEnabled("cmd_removecomsa", true);
	Button.SetEnabled("cmd_removeincomsa", true);
	Button.SetEnabled("cmd_remover", true);
	ListBox.SetEnabled("ltb_comsa", true);
	ListBox.SetEnabled("ltb_incomsa", true);
	RadioButton.SetEnabled("rdb_askallsa", true);
	RadioButton.SetEnabled("rdb_askofflistsa", true);
	RadioButton.SetEnabled("rdb_offallsa", true);
	RadioButton.SetEnabled("rdb_onallsa", true);
	exportdir = gtadir;
	loadnplugin();
end
function switchOFF()
	Button.SetEnabled("cmd_addcomsa", false);
	Button.SetEnabled("cmd_addincomsa", false);
	Button.SetEnabled("cmd_apli", true);
	Button.SetEnabled("cmd_helpgta", true);
	Button.SetEnabled("cmd_instalar", false);
	Button.SetEnabled("cmd_jogaragora", false);
	Button.SetEnabled("cmd_limpacomsa", false);
	Button.SetEnabled("cmd_limpaincomsa", false);
	Button.SetEnabled("cmd_makeatalho", false);
	Button.SetEnabled("cmd_ver", false);
	Button.SetEnabled("cmd_removecomsa", false);
	Button.SetEnabled("cmd_removeincomsa", false);
	Button.SetEnabled("cmd_remover", false);
	ListBox.SetEnabled("ltb_comsa", false);
	ListBox.SetEnabled("ltb_incomsa", false);
	RadioButton.SetEnabled("rdb_askallsa", false);
	RadioButton.SetEnabled("rdb_askofflistsa", false);
	RadioButton.SetEnabled("rdb_offallsa", false);
	RadioButton.SetEnabled("rdb_onallsa", false);
	Paragraph.SetText("prg_plugins", "Plug-ins ASI instalados: 0\r\nPlugins CLEO instalados: 0\r\nTotal de plug-ins instalados: 0");
end

function carregaSA()
	if sainit == 01 then
		RadioButton.SetChecked("rdb_offallsa", true);
	end
	if sainit == 02 then
		RadioButton.SetChecked("rdb_onallsa", true);
	end
	if sainit == 03 then
		RadioButton.SetChecked("rdb_askofflistsa", true);
	end
	if sainit == 04 then
		RadioButton.SetChecked("rdb_askallsa", true);
	end
	Input.SetText("txt_gtadir", gtadir);
	--listcomsa
	lidoincom = TextFile.ReadToTable("Mod_Locker\\SA_listcom.db");
	if lidoincom[4] ~= "" then
		Table.Insert(lidoincom, 4, "");
	end
	totallido = Table.Count(lidoincom);
	numero = 5;
	if lidoincom[4] ~= "" then
		Table.Insert(lidoincom, 4, "");
	end
	if totallido &gt; 4 then
		for x = 5,totallido do
			ListBox.AddItem("ltb_comsa", lidoincom[x], "");
		end
	end
	--fim
	--listincomsa
	lidocom = TextFile.ReadToTable("Mod_Locker\\SA_listincom.db");
	if lidocom[4] ~= "" then
		Table.Insert(lidocom, 4, "");
	end
	totallido = Table.Count(lidocom);
	numero = 5;
	if totallido &gt; 4 then
		for x = 5,totallido do
			ListBox.AddItem("ltb_incomsa", lidocom[x], "");
		end
	end
	--fim
	inittimesa = inittimesa + 1;
end

Load_dados();
ChangeWindowType();
transparency = 255;
while transparency &gt;= 245 do
	transparency = transparency - 1
	SetWindowTransparency(transparency);
	Application.Sleep(40);
end
exportdir = gtadir;
if inittimesa == 0 then
	if gtadir ~= "" then
		switchON();
	end
	carregaSA();
end

Image.Load("cmd_gtasa", "Mod_Locker\\imagens\\gtahigh.png");
Image.Load("cmd_samp", "Mod_Locker\\imagens\\sampnormal.png");
Image.Load("img_main", "Mod_Locker\\imagens\\wml_normal.png");
Image.Load("cmd_mtasa", "Mod_Locker\\imagens\\mtasanormal.png");

if gtadir == "" then
	switchOFF();
end
loadnplugin();
</Script>
