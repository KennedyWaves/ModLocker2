	DialogEx.Show("Any_msg", true, nil, nil);
	if resp_msg == 1 then
		TextFile.WriteFromTable("Mod_Locker\\SAMP_listincom.db", gravaincom, false);
	end
else
	TextFile.WriteFromTable("Mod_Locker\\SAMP_listincom.db", gravaincomsamp, false);
end

--listcomsa
gravacomsamp = {lidocomsamp[1],lidocomsamp[2],lidocomsamp[3],""}
numerocomsamp = 5;
totalcomsamp = ListBox.GetCount("ltb_comsa");
for x=1,totalcomsamp do
	textcomsamp = ListBox.GetItemText("ltb_comsa", x);
	Table.Insert(gravacomsamp, Table.Count(gravacomsamp)+1, textcomsamp);
end

if gravacomsamp[4] ~= "" then
	Table.Insert(gravacomsamp, 4, "");
end

totalgravasamp = Table.Count(gravacomsamp);
compatsamp = TextFile.ReadToString("Mod_Locker\\SAMP_listincom.db");
erro = 0;
for x = 5,totalgravasamp do
	if gravacomsamp[x] then
		esta = String.Find(compatsamp, gravacomsamp[x], 1, false);
		if esta ~= -1 then
			erro = erro + 1;
		end
	end
end
if totalgravasamp &lt; 5 then
	erro = 0;
end
if erro ~= 0 then
	img_msg = 3;
	txt_msg = "Um ou mais plug-ins desta lista estão incluídos na lista de plug-ins incompatíveis. Deseja salvar assim mesmo?";
	enablecmd1 = true;
	enablecmd2 = true;
	one_cmdtext = "SIM";
	two_cmdtext = "NÃO";
	DialogEx.Show("Any_msg", true, nil, nil);
	if resp_msg == 1 then
		TextFile.WriteFromTable("Mod_Locker\\SAMP_listcom.db", gravacomsamp, false);
		apl();
	end
else
	TextFile.WriteFromTable("Mod_Locker\\SAMP_listcom.db", gravacomsamp, false);
	apl();
end

Shell.Execute("Mod_Locker.exe", "open", "/samp", "", SW_SHOWNORMAL, false);
