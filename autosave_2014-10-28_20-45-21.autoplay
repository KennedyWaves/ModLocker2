function SAMP_Launcher()
	Label.SetText("LBL_MSG", "Bloqueando Plug-ins...");	
	if sampdir == "" then
		img_msg = 4;
		txt_msg = "Não foi definido um diretório para o SAMP! Configure no Mod Locker!";
		enablecmd1 = false;
		enablecmd2 = true;
		one_cmdtext = "";
		two_cmdtext = "OK";
		DialogEx.Show("Any_msg", true, nil, nil);
		Shell.Execute("Mod_Locker.exe", "open", "", "", SW_SHOWNORMAL, false);
		DialogEx.Close(0);
	else
		chkgta = File.Find(sampdir, "gta_sa.exe", false, false, nil, nil);
		if chkgta then
			Shell.Execute("Adm_Locker.exe", "open", "/samp", "", SW_SHOWNORMAL, true);
			off = Application.LoadValue("Adm_Locker", "Force off");
			if off == "off" then
				DialogEx.Close(0);
				Application.Exit(0);
			end
			Shell.Execute(""..sampdir.."\\samp.exe", "open", "", "", SW_SHOWNORMAL, false);
		else
			img_msg = 4;
			txt_msg = "O executável \"gta_sa.exe\" não foi encontrado!";
			enablecmd1 = false;
			enablecmd2 = true;
			one_cmdtext = "";
			two_cmdtext = "OK";
			DialogEx.Show("Any_msg", true, nil, nil);
		end
		DialogEx.Close(0);
		Application.Exit(0);
	end
end


