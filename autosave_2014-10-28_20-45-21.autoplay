		if (sampoffall) then
			savecfgsamp = 01;
		end
		if (samponall) then
			savecfgsamp = 02;
		end
		if (sampaskofflist) then
			savecfgsamp = 03;
		end
		if (sampaskall) then
			savecfgsamp = 04;
		end
		loadedsampdir = Input.GetText("txt_sampdir");
		ipsum = "\"ipsum\"";
		readsampdir = String.Replace(ipsum, "ipsum", loadedsampdir, false);
		readsampdir = String.Replace(readsampdir, "\\", "\\\\", false);
		loadcfgpresampdir = "sampdir = "..readsampdir..";";
		loadcfgpresamp = "sampinit = 0"..savecfgsamp..";";
		loadcfg[6] = loadcfgpresamp;
		loadcfg[12] = loadcfgpresampdir;
	end
		if page == "mtasa" then
		sampoffall = RadioButton.GetChecked("rdb_offallmta");
		samponall = RadioButton.GetChecked("rdb_onallmta");
		sampaskofflist = RadioButton.GetChecked("rdb_askofflistmta");
		sampaskall = RadioButton.GetChecked("rdb_askallmta");
		if (sampoffall) then
			savecfgsamp = 01;
		end
		if (samponall) then
			savecfgsamp = 02;
		end
		if (sampaskofflist) then
			savecfgsamp = 03;
		end
		if (sampaskall) then
			savecfgsamp = 04;
		end
		loadedsampdir = Input.GetText("txt_mtasadir");
		ipsum = "\"ipsum\"";
		readsampdir = String.Replace(ipsum, "ipsum", loadedsampdir, false);
		readsampdir = String.Replace(readsampdir, "\\", "\\\\", false);
		loadcfgpresampdir = "mtadir = "..readsampdir..";";
		loadcfgpresamp = "mtainit = 0"..savecfgsamp..";";
		loadedmtaexedir = Input.GetText("txt_mtaexedir");
		ipsum = "\"ipsum\"";
		readmtaexedir = String.Replace(ipsum, "ipsum", loadedmtaexedir, false);
		readmtaexedir = String.Replace(readmtaexedir, "\\", "\\\\", false);
		loadcfgpremtaexedir = "mtaexedir = "..readmtaexedir..";";
		loadcfg[7] = loadcfgpresamp;
		loadcfg[13] = loadcfgpresampdir;
		loadcfg[17] = loadcfgpremtaexedir;
	end
	TextFile.WriteFromTable("Mod_Locker\\MOD_LOCKER.cfg", loadcfg, false);
	Button.SetEnabled("cmd_apli", false);
end

