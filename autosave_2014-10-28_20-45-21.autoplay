<Event>
<Name>On Show</Name>
<Args/>
<Script>if (fecha) then
	Application.Exit(0);
end
ChangeWindowType();
while transparency &gt;= 245 do
	transparency = transparency - 1
	SetWindowTransparency(transparency);
	Application.Sleep(40);
end
hWnd = Application.GetWndHandle();
nExisitngStyle = DLL.CallFunction("User32.dll", "GetClassLongA", hWnd..",-26", DLL_RETURN_TYPE_LONG, DLL_CALL_STDCALL);

nNewStyle = nExisitngStyle+131072;
DLL.CallFunction("User32.dll", "SetClassLongA", hWnd..",-26,"..nNewStyle, DLL_RETURN_TYPE_LONG, DLL_CALL_STDCALL);
Load_dados();
if gtadir == "" then
	Button.SetEnabled("cmd_jogarsa", false);
	Button.SetEnabled("cmd_plsa", false);
	Button.SetVisible("cmd_plsa", false);
else
	Button.SetEnabled("cmd_jogarsa", true);
	Button.SetEnabled("cmd_plsa", true);
	Button.SetVisible("cmd_plsa", true);
end
if sampdir == "" then
	Button.SetEnabled("cmd_jogarsamp", false);
	Button.SetEnabled("cmd_plsamp", false);
	Button.SetVisible("cmd_plsamp", false);
else
	Button.SetEnabled("cmd_jogarsamp", true);
	Button.SetEnabled("cmd_plsamp", true);
	if sampdir == gtadir then
		Button.SetVisible("cmd_plsamp", false);
	else
		Button.SetVisible("cmd_plsamp", true);
	end
end
if mtadir == "" then
	Button.SetEnabled("cmd_jogarmtasa", false);
	Button.SetVisible("cmd_plmtasa", false);
	Button.SetEnabled("cmd_plmtasa", false);
else
	Button.SetEnabled("cmd_jogarmtasa", true);
	Button.SetEnabled("cmd_plmtasa", true);
	if mtadir == gtadir then
		Button.SetVisible("cmd_plmtasa", false);
	else
		Button.SetVisible("cmd_plmtasa", true);
	end
end
if mtadir == "" and gtadir == "" and sampdir == "" then
	Button.SetEnabled("cmd_makesupsa", false);
	Button.SetVisible("cmd_makesupsa", false);
else
	Button.SetEnabled("cmd_makesupsa", true);
	Button.SetVisible("cmd_makesupsa", true);
end
startlist = Application.LoadValue("Mod_Locker", "Start");
if startlist ~= "" then
	startlist = String.ToNumber(startlist);
	ComboBox.SetSelected("cbb_tipoinit", startlist);
end
Image.Load("img_main", "Mod_Locker\\imagens\\wml_normal.png");
Image.Load("cmd_gtasa", "Mod_Locker\\imagens\\gtanormal.png");
Image.Load("cmd_samp", "Mod_Locker\\imagens\\sampnormal.png");
Image.Load("cmd_mtasa", "Mod_Locker\\imagens\\mtasanormal.png");
</Script>
