tblMenu[1].Text = "&amp;Sair	Alt+F4";
tblMenu[1].ID = 100;
tblMenu[1].IconID = 0;
tblMenu[1].Checked = false;
tblMenu[1].Enabled = true;
tblMenu[2] ={};
tblMenu[2].Text = "&amp;Minimizar";
tblMenu[2].ID = 200;
tblMenu[2].IconID = 0;
tblMenu[2].Checked = false;
tblMenu[2].Enabled = true;
mousePos = System.GetMousePosition(true, nil);
POP = Application.ShowPopupMenu((mousePos.X-178), mousePos.Y, tblMenu, TPM_LEFTALIGN, TPM_TOPALIGN, true, true);
if(POP == 100)then
	while transparency &gt; 121 do
		transparency = transparency - 12
		SetWindowTransparency(transparency);
		Application.Sleep(20);
	end
	DialogEx.Show("tks", true, nil, nil);
	Application.Exit(0);	
end
 
if(POP == 200)then
	Application.Minimize();
end</Script>
