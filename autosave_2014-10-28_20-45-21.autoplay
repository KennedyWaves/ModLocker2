		loadcfgpresa = "sainit = 0"..savecfgsa..";";
		loadcfg[5] = loadcfgpresa;
		loadcfg[11] = loadcfgpregtadir;
	end
	if page == "samp" then
		sampoffall = RadioButton.GetChecked("rdb_offallsamp");
		samponall = RadioButton.GetChecked("rdb_onallsamp");
		sampaskofflist = RadioButton.GetChecked("rdb_askofflistsamp");
