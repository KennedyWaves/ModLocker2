<Script>page = "gta";
--listincom
gravaincom = {lidoincom[1],lidoincom[2],lidoincom[3],""}
numeroincom = 5;
totalincom = ListBox.GetCount("ltb_incomsa");
for x=1,totalincom do
	textincom = ListBox.GetItemText("ltb_incomsa", x);
	Table.Insert(gravaincom, Table.Count(gravaincom)+1, textincom);
end

if gravaincom[4] ~= "" then
	Table.Insert(gravaincom, 4, "");
end

totalgrava = Table.Count(gravaincom);
compat = TextFile.ReadToString("Mod_Locker\\SA_padcom.db");
erro = 0;
for x = 5,totalgrava do
	if gravaincom[x] then
		esta = String.Find(compat, gravaincom[x], 1, false);
		if esta ~= -1 then
			erro = erro + 1;
		end
	end
end
if totalgrava &lt; 5 then
	erro = 0;
end
if erro ~= 0 then
	img_msg = 3;
	txt_msg = "Um ou mais plug-ins desta lista estão incluídos na lista padrão de plug-ins compatíveis. Deseja salvar assim mesmo?";
	enablecmd1 = true;
	enablecmd2 = true;
	one_cmdtext = "SIM";
	two_cmdtext = "NÃO";
	DialogEx.Show("Any_msg", true, nil, nil);
	if resp_msg == 1 then
		TextFile.WriteFromTable("Mod_Locker\\SA_listincom.db", gravaincom, false);
	end
else
	TextFile.WriteFromTable("Mod_Locker\\SA_listincom.db", gravaincom, false);
end

--listcomsa
gravacom = {lidocom[1],lidocom[2],lidocom[3],""}
numerocom = 5;
totalcom = ListBox.GetCount("ltb_comsa");
for x=1,totalcom do
	textcom = ListBox.GetItemText("ltb_comsa", x);
	Table.Insert(gravacom, Table.Count(gravacom)+1, textcom);
end

if gravacom[4] ~= "" then
	Table.Insert(gravacom, 4, "");
end

totalgrava = Table.Count(gravacom);
compat = TextFile.ReadToString("Mod_Locker\\SA_listincom.db");
erro = 0;
for x = 5,totalgrava do
	if gravacom[x] then
		esta = String.Find(compat, gravacom[x], 1, false);
		if esta ~= -1 then
			erro = erro + 1;
		end
	end
end
if totalgrava &lt; 5 then
	erro = 0;
end
if erro ~= 0 then
	img_msg = 3;
	txt_msg = "Um ou mais plug-ins desta lista estão incluídos na lista de plug-ins incompatíveis. Deseja salvar assim mesmo?";
	enablecmd1 = true;
	enablecmd2 = true;
	one_cmdtext = "SIM";
	two_cmdtext = "NÃO";
	DialogEx.Show("Any_msg", true, nil, nil);
	if resp_msg == 1 then
		TextFile.WriteFromTable("Mod_Locker\\SA_listcom.db", gravacom, false);
		apl();
	end
else
	TextFile.WriteFromTable("Mod_Locker\\SA_listcom.db", gravacom, false);
	apl();
end

Shell.Execute("Mod_Locker.exe", "open", "/sa", "", SW_SHOWNORMAL, false);
Application.Exit(0);</Script>
