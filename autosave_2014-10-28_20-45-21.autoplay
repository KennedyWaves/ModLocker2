erro = 0;
for x = 5,totalgravamtasa do
	if gravacommtasa[x] then
		esta = String.Find(compatmtasa, gravacommtasa[x], 1, false);
		if esta ~= -1 then
			erro = erro + 1;
		end
	end
end
if totalgravamtasa &lt; 5 then
	erro = 0;
end
if erro ~= 0 then
	img_msg = 3;
