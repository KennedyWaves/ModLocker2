<Script>function switchON()
	Shell.Execute("Adm_Locker.exe", "open", "/mtasaunlock", "", SW_SHOWNORMAL, true);
	Button.SetEnabled("cmd_addcomsa", true);
	Button.SetEnabled("cmd_mtadir", true);
	Button.SetEnabled("cmd_addincomsa", true);
	Button.SetEnabled("cmd_helpgta", true);
	Button.SetEnabled("cmd_instalar", true);
	Button.SetEnabled("cmd_jogaragora", true);
	Button.SetEnabled("cmd_limpacomsa", true);
	Button.SetEnabled("cmd_limpaincomsa", true);
	Button.SetEnabled("cmd_makeatalho", true);
	Button.SetEnabled("cmd_ver", true);
	Button.SetEnabled("cmd_vergta", true);
	Button.SetEnabled("cmd_removecomsa", true);
	Button.SetEnabled("cmd_removeincomsa", true);
	Button.SetEnabled("cmd_remover", true);
	ListBox.SetEnabled("ltb_comsa", true);
	ListBox.SetEnabled("ltb_incomsa", true);
	RadioButton.SetEnabled("rdb_askallmta", true);
	RadioButton.SetEnabled("rdb_askofflistmta", true);
	RadioButton.SetEnabled("rdb_offallmta", true);
	RadioButton.SetEnabled("rdb_onallmta", true);
	exportdir = mtadir;
	loadnplugin();
end
	
function switchOFF()
	Button.SetEnabled("cmd_addcomsa", false);
	Button.SetEnabled("cmd_addincomsa", false);
	Button.SetEnabled("cmd_apli", true);
	Button.SetEnabled("cmd_helpgta", true);
	Button.SetEnabled("cmd_instalar", false);
	Button.SetEnabled("cmd_jogaragora", false);
	Button.SetEnabled("cmd_vergta", false);
	Button.SetEnabled("cmd_limpacomsa", false);
	Button.SetEnabled("cmd_limpaincomsa", false);
	Button.SetEnabled("cmd_makeatalho", false);
	Button.SetEnabled("cmd_ver", false);
	Button.SetEnabled("cmd_removecomsa", false);
	Button.SetEnabled("cmd_removeincomsa", false);
	Button.SetEnabled("cmd_remover", false);
	ListBox.SetEnabled("ltb_comsa", false);
	ListBox.SetEnabled("ltb_incomsa", false);
	RadioButton.SetEnabled("rdb_askallmta", false);
	RadioButton.SetEnabled("rdb_askofflistmta", false);
	RadioButton.SetEnabled("rdb_offallmta", false);
	RadioButton.SetEnabled("rdb_onallmta", false);
	Paragraph.SetText("prg_plugins", "Plug-ins ASI instalados: 0\r\nPlugins CLEO instalados: 0\r\nTotal de plug-ins instalados: 0");
end

function carregamtasa()
	if mtainit == 01 then
		RadioButton.SetChecked("rdb_offallmta", true);
	end
	if mtainit == 02 then
		RadioButton.SetChecked("rdb_onallmta", true);
	end
	if mtainit == 03 then
		RadioButton.SetChecked("rdb_askofflistmta", true);
	end
	if mtainit == 04 then
		RadioButton.SetChecked("rdb_askallmta", true);
	end
	Input.SetText("txt_mtasadir", mtadir);
	Input.SetText("txt_mtaexedir", mtaexedir);
	--listcomsa
	lidoincommtasa = TextFile.ReadToTable("Mod_Locker\\mtasa_listcom.db");
	totallidomtasa = Table.Count(lidoincommtasa);
	numero = 5;
	if lidoincommtasa[4] ~= "" then
		Table.Insert(lidoincommtasa, 4, "");
	end
	if totallidomtasa &gt; 4 then
		for x = 5,totallidomtasa do
			ListBox.AddItem("ltb_comsa", lidoincommtasa[x], "");
		end
	end
	--fim
	--listincomsa
	lidocommtasa = TextFile.ReadToTable("Mod_Locker\\mtasa_listincom.db");
	local totallidomtasa = Table.Count(lidocommtasa);
	numero = 5;
	if lidocommtasa[4] ~= "" then
		Table.Insert(lidocommtasa, 4, "");
	end
	if totallidomtasa &gt; 4 then
		for x = 5,totallidomtasa do
			ListBox.AddItem("ltb_incomsa", lidocommtasa[x], "");
		end
	end
	--fim
	inittimemtasa = inittimemtasa + 1;
end

Load_dados();
ChangeWindowType();
transparency = 255;
while transparency &gt;= 245 do
	transparency = transparency - 1
	SetWindowTransparency(transparency);
	Application.Sleep(40);
end
exportdir = mtadir;
if inittimemtasa == 0 then
	if mtadir ~= "" and mtaexedir ~= "" then
		switchON();
	end
	carregamtasa();
end

Image.Load("cmd_mtasa", "Mod_Locker\\imagens\\mtasahigh.png");
Image.Load("cmd_gtasa", "Mod_Locker\\imagens\\gtanormal.png");
Image.Load("cmd_samp", "Mod_Locker\\imagens\\sampnormal.png");
Image.Load("img_main", "Mod_Locker\\imagens\\wml_normal.png");


if mtadir == "" or mtaexedir == "" then
	switchOFF();
end

loadnplugin();</Script>
