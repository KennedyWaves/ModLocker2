<Script>Label.SetText("LBL_MSG", "Iniciando...");
Load_dados();
fecha = false;
for i, Arg in pairs (_CommandLineArgs) do
	if (String.CompareNoCase(Arg, "/samp") == 0) then
		fecha = true;
		SAMP_Launcher();
	end
	if (String.CompareNoCase(Arg, "/vcmp") == 0) then
		fecha = true;
		VCMP_Launcher();
	end
	if (String.CompareNoCase(Arg, "/sa") == 0) then
		fecha = true;
		SA_Launcher();
	end
	if (String.CompareNoCase(Arg, "/vc") == 0) then
		fecha = true;
		VC_Launcher();
	end
	if (String.CompareNoCase(Arg, "/mtasa") == 0) then
		fecha = true;
		MTASA_Launcher();
	end
	if (String.CompareNoCase(Arg, "/mtavc") == 0) then
		fecha = true;
		MTAVC_Launcher();
	end
end
if	fecha == false then
	Application.Sleep(2000);
end
DialogEx.Close(0);
</Script>
