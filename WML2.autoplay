<Event>
<Name>On Click</Name>
<Args/>
<Script>loadedmtadir = Dialog.FolderBrowse("Selecione o diretório do MTA San Andreas:", arqprog);
if (loadedmtadir) then
	if loadedmtadir ~= "CANCEL" then
		Input.SetText("txt_mtadir", loadedmtadir);
		mtadir = loadedmtadir;
		Button.SetEnabled("cmd_apli", true);
	end
end
mtaexedir = Input.GetText("txt_mtaexedir");
mtadir = loadedmtadir;
if mtadir == "" or mtaexedir == "" then
	mtaswitchOFF();
else
	mtaswitchON();
end
chkgta = File.Find(mtadir, "gta_sa.exe", false, false, nil, nil);
if chkgta == nil then
	img_msg = 4;
	txt_msg = "O executável \"gta_sa.exe\" não foi encontrado!";
	enablecmd1 = false;
	enablecmd2 = true;
	one_cmdtext = "";
	two_cmdtext = "OK";
	DialogEx.Show("Any_msg", true, nil, nil);
	mtaswitchOFF();
end</Script>
