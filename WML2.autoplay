function carregamtasa()
	if mtainit == 01 then
		RadioButton.SetChecked("rdb_offallmta", true);
	end
	if mtainit == 02 then
		RadioButton.SetChecked("rdb_onallmta", true);
	end
	if mtainit == 03 then
		RadioButton.SetChecked("rdb_askofflistmta", true);
	end
	if mtainit == 04 then
		RadioButton.SetChecked("rdb_askallmta", true);
	end
	Input.SetText("txt_mtasadir", mtadir);
	Input.SetText("txt_mtaexedir", mtaexedir);
	--listincomsa
	lidoincommtasa = TextFile.ReadToTable("Mod_Locker\\mtasa_listincom.db");
	if (lidoincommtasa) then
		totallidomtasa = Table.Count(lidoincommtasa);
		numero = 5;
		if lidoincommtasa[4] ~= "" then
			Table.Insert(lidoincommtasa, 4, "");
		end
		if totallidomtasa &gt; 4 then
			for x = 5,totallidomtasa do
				ListBox.AddItem("ltb_comsa", lidoincommtasa[x], "");
			end
		end
	else
		TextFile.WriteFromString("Mod_Locker\\MTASA_listincom.db", "--[[================WAVES INFORMATICA================\r\n======DEFINICAO PERSONALIZADA DE PLUG-INS INCOMPATIVEIS==\r\n==============MULTI THEFT AUTO:SAN ANDREAS=======]]--\r\n", false);
		lidoincommtasa = TextFile.ReadToTable("Mod_Locker\\mtasa_listincom.db");
	end
	--fim
	--listcomsa
	lidocommtasa = TextFile.ReadToTable("Mod_Locker\\mtasa_listcom.db");
	if (lidocommtasa) then
		local totallidomtasa = Table.Count(lidocommtasa);
		numero = 5;
		if lidocommtasa[4] ~= "" then
			Table.Insert(lidocommtasa, 4, "");
		end
		if totallidomtasa &gt; 4 then
			for x = 5,totallidomtasa do
				ListBox.AddItem("ltb_incomsa", lidocommtasa[x], "");
			end
		end
	else
		TextFile.WriteFromString("Mod_Locker\\MTASA_listcom.db", "--[[================WAVES INFORMATICA================\r\n======DEFINICAO PERSONALIZADA DE PLUG-INS COMPATIVEIS==\r\n==============MULTI THEFT AUTO:SAN ANDREAS=======]]--\r\n", false);
		lidocommtasa = TextFile.ReadToTable("Mod_Locker\\mtasa_listcom.db");
	end
	--fim
	chkpad = File.Find("Mod_Locker", "MTA_padcom.db", false, false, nil, nil);
	if chkpad == nil then
		TextFile.WriteFromString("Mod_Locker\\MTASA_padcom.db", "--[[================WAVES INFORMATICA================\r\n==========DEFINICAO PADRAO DE MODS COMPATIVEIS=======\r\n==============MULTI THEFT AUTO:SAN ANDREAS=======]]--\r\n\r\nImVehFT.asi\r\nDSS.cs\r\nMemory512.cs\r\nStreamMemFix1.0.asi\r\nStreamMemFix2.2_test1.asi\r\nStreamMemFix2.2_test2.asi\r\nCLEO.asi\r\nradarrect.asi\r\n\r\n", false);
	end	
	inittimemtasa = inittimemtasa + 1;
end

Load_dados();
ChangeWindowType();
transparency = transparency_set;
SetWindowTransparency(transparency);
exportdir = mtadir;
if inittimesa == 0 or (String.CompareNoCase(mtadir, lastmtadir)== -1) or (String.CompareNoCase(mtaexedir, lastmtaexedir)~= 0) then
	if mtadir ~= "" and mtaexedir ~= "" then
		switchON();
	end
	carregamtasa();
end

Image.Load("cmd_mtasa", "Mod_Locker\\imagens\\mtasahigh.png");
Image.Load("cmd_gtasa", "Mod_Locker\\imagens\\gtanormal.png");
Image.Load("cmd_samp", "Mod_Locker\\imagens\\sampnormal.png");
Image.Load("img_main", "Mod_Locker\\imagens\\wml_normal.png");


if mtadir == "" or mtaexedir == "" then
	switchOFF();
end

loadnplugin();</Script>
