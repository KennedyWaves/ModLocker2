<Args>number e_Key, table e_Modifiers</Args>
<Script>Button.SetEnabled("cmd_apli", true);
if (e_Key == 13) then
	gtadir = Input.GetText("txt_gtadir");
	Button.SetEnabled("cmd_apli", true);
	if gtadir == "" then
		switchOFF();
	else
		chkgta = File.Find(gtadir, "*gta_sa.exe", false, false, nil, nil);
		if chkgta then
			switchON();
		else
			img_msg = 4;
			txt_msg = "O executável \"gta_sa.exe\" não foi encontrado!";
			enablecmd1 = false;
			enablecmd2 = true;
			one_cmdtext = "";
			two_cmdtext = "OK";
			DialogEx.Show("Any_msg", true, nil, nil);
			switchOFF();
		end
	end
end	</Script>
