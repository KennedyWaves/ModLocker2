function loadnplugin()
	local cleo = File.Find(""..exportdir.."\\cleo", "*.cs", false, false, nil, nil);
	local asi = File.Find(exportdir, "*.asi", false, false, nil, nil);
	local totalcleo = 0;
	local totalasi = 0;
	local totalmods = 0;
	if (cleo) then
		totalcleo = Table.Count(cleo);
		totalmods = totalmods + totalcleo;
	end
	if (asi) then
		totalasi = Table.Count(asi);
		totalmods = totalmods + totalasi;
	end
	Paragraph.SetText("prg_plugins", "Plug-ins ASI instalados: "..totalasi.."\r\nPlugins CLEO instalados: "..totalcleo.."\r\nTotal de plug-ins instalados: "..totalmods);
end

function diagplug()
	local cleo = File.Find(""..exportdir.."\\cleo", "*.cs", false, false, nil, nil);
	local asi = File.Find(exportdir, "*.asi", false, false, nil, nil);
	local totalcleo = 0;
	local totalasi = 0;
	local totalmods = 0;
	if (cleo) then
		totalcleo = Table.Count(cleo);
		totalmods = totalmods + totalcleo;
	end
	if (asi) then
		totalasi = Table.Count(asi);
		totalmods = totalmods + totalasi;
	end
	nsetplug = "CLEO: "..totalcleo.." | ASI: "..totalasi.." | Total: "..totalmods.."";
end

function botao_exit()
	tblMenu = {};
	tblMenu[1] ={};
	tblMenu[1].Text = "&amp;Sair	Alt+F4";
	tblMenu[1].ID = 100;
	tblMenu[1].IconID = 0;
	tblMenu[1].Checked = false;
	tblMenu[1].Enabled = true;
	tblMenu[2] ={};
	tblMenu[2].Text = "&amp;Minimizar";
	tblMenu[2].ID = 200;
	tblMenu[2].IconID = 0;
	tblMenu[2].Checked = false;
	tblMenu[2].Enabled = true;
	mousePos = System.GetMousePosition(true, nil);
	POP = Application.ShowPopupMenu((mousePos.X-178), mousePos.Y, tblMenu, TPM_LEFTALIGN, TPM_TOPALIGN, true, true);
	if(POP == 100)then
		Button.SetEnabled("cmd_off", false);
		while transparency &gt; 121 do
			transparency = transparency - 12
			SetWindowTransparency(transparency);
			Application.Sleep(20);
		end
		DialogEx.Show("tks", true, nil, nil);
		Application.Exit(0);	
	end
	 
	if(POP == 200)then
		Button.SetEnabled("cmd_off", false);
		while transparency &gt; 0 do
			transparency = transparency - 12
			if transparency &lt; 0 then
				transparency = 0
			end
			SetWindowTransparency(transparency);
			Application.Sleep(20);
		end
		Application.Minimize();
		SetWindowTransparency(transparency_set);
		transparency = transparency_set;
		Button.SetEnabled("cmd_off", true);
	end
