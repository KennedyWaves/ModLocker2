function Load_dados()
	usadodir = _SourceFolder
	arqprog = Shell.GetFolder(SHF_PROGRAMFILES);
	sainit = INIFile.GetValue("Mod_Locker\\MOD_LOCKER.INI", "INIT", "sainit");
	sainit = String.ToNumber(sainit);
	sampinit = INIFile.GetValue("Mod_Locker\\MOD_LOCKER.INI", "INIT", "sampinit");
	sampinit = String.ToNumber(sampinit);
	mtainit = INIFile.GetValue("Mod_Locker\\MOD_LOCKER.INI", "INIT", "mtainit");
	mtainit = String.ToNumber(mtainit);
	if sainit &lt; 1 then
		sainit = 3;
	end
	if sampinit &lt; 1 then
		sampinit = 3;
	end
	if mtainit &lt; 1 then
		mtainit = 3;
	end
	gtadir = INIFile.GetValue("Mod_Locker\\MOD_LOCKER.INI", "DIR", "gtadir");
	sampdir = INIFile.GetValue("Mod_Locker\\MOD_LOCKER.INI", "DIR", "sampdir");
	mtadir = INIFile.GetValue("Mod_Locker\\MOD_LOCKER.INI", "DIR", "mtadir");
	mtaexedir = INIFile.GetValue("Mod_Locker\\MOD_LOCKER.INI", "DIR", "mtaexedir");
	transparency_set = INIFile.GetValue("Mod_Locker\\MOD_LOCKER.INI", "GERAL", "transparency_set");
	transparency_set = String.ToNumber(transparency_set);
	if transparency_set &lt; 102 or transparency_set &gt; 255 then
		transparency_set = 240;
	end
end

function SAMP_Launcher()
	Application.SaveValue("Adm_Locker", "Force off", "");
	Label.SetText("LBL_MSG", "Bloqueando Plug-ins...");	
	if sampdir == "" then
		img_msg = 4;
		txt_msg = "Não foi definido um diretório para o SAMP! Configure no Mod Locker!";
		enablecmd1 = false;
		enablecmd2 = true;
		one_cmdtext = "";
		two_cmdtext = "OK";
		DialogEx.Show("Any_msg", true, nil, nil);
		Shell.Execute("Mod_Locker.exe", "open", "", "", SW_SHOWNORMAL, false);
		DialogEx.Close(0);
	else
		chkgta = File.Find(sampdir, "gta_sa.exe", false, false, nil, nil);
		if chkgta then
			Shell.Execute("Adm_Locker.exe", "open", "/samp", "", SW_SHOWNORMAL, true);
			off = Application.LoadValue("Adm_Locker", "Force off");
			if off == "off" then
				DialogEx.Close(0);
				Application.Exit(0);
			end
			Shell.Execute(sampdir.."\\samp.exe", "open", "", sampdir, SW_SHOWNORMAL, false);
		else
			img_msg = 4;
			txt_msg = "O executável \"gta_sa.exe\" não foi encontrado!";
			enablecmd1 = false;
			enablecmd2 = true;
			one_cmdtext = "";
			two_cmdtext = "OK";
			DialogEx.Show("Any_msg", true, nil, nil);
		end
		DialogEx.Close(0);
		Application.Exit(0);
	end
end


