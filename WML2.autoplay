<Script>Button.SetEnabled("cmd_apli", true);
if (e_Key == 13) then
	sampdir = Input.GetText("txt_sampdir");
	Button.SetEnabled("cmd_apli", true);
	if sampdir == "" then
		switchOFF();
	else
		chkgta = File.Find(sampdir, "gta_sa.exe", false, false, nil, nil);
