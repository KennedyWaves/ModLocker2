<Script>arqprog = Shell.GetFolder(SHF_PROGRAMFILES);
loadedsampdir = Dialog.FolderBrowse("Selecione o diretório do San Andreas MultiPlayer:", arqprog);
if (loadedsampdir) then
	if loadedsampdir ~= "CANCEL" then
