listconcattira = String.Concat(listconcattira, "\r\n"..concatlidocom.."");
DialogEx.Show("add_plugin", true, nil, nil);
if addplugin ~= "cancelado" then
	for x,y in pairs (selec_table) do
		ListBox.AddItem("ltb_comsa", selec_table[x], "");
	end
end
Button.SetEnabled("cmd_apli", true);
</Script>
