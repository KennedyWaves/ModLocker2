<Event>
<Name>On Right-Click</Name>
<Args/>
<Script>chektodos = CheckBox.GetChecked("chk_todos");
if chektodos then
	Xaddplugin();
else
	function XaddpluginAll()
		ListBox.DeleteItem(setlistbox, -1);
		lidocleo = File.Find(""..gtadir.."\\cleo", "*.cs", false, false, nil, nil);
		totallido = Table.Count(lidocleo);
		for x,y in pairs (lidocleo) do
			nomeshow = lidocleo[x];
			nomeshow = String.Right(nomeshow, (String.Length(nomeshow) - String.ReverseFind(nomeshow, "\\", true)));
			ListBox.AddItem(setlistbox, nomeshow, "");
		end
		lidotable = File.Find(gtadir, "*.asi", false, false, nil, nil);
		totallido = Table.Count(lidotable);
		for x,y in pairs (lidotable) do
			nomeshow = lidotable[x];
			nomeshow = String.Right(nomeshow, (String.Length(nomeshow) - String.ReverseFind(nomeshow, "\\", true)));
			ListBox.AddItem(setlistbox, nomeshow, "");
		end
	end
	XaddpluginAll();
end
</Script>
