<Script>chkgta = File.Find(sampdir, "gta_sa.exe", false, false, nil, nil);
if chkgta then
	exportdir = sampdir;
	listconcattira = "";
	lidopadcom = TextFile.ReadToTable("Mod_Locker\\SAMP_padcom.db");
	totallido = Table.Count(lidopadcom);
	if lidopadcom[4] ~= "" then
		Table.Insert(lidopadcom, 4, "");
	end
	concatlidocom = Table.Concat(lidopadcom, "\r\n", 1, TABLE_ALL);
	listconcattira = String.Concat(listconcattira, "\r\n"..concatlidocom.."");
	Shell.Execute("Adm_Locker.exe", "open", "/sampunlock", "", SW_SHOWNORMAL, true);
	DialogEx.Show("allplguins", true, nil, nil);
else
	img_msg = 4;
	txt_msg = "O diretório definido para San Andreas MultiPlayer não é inválido!";
	enablecmd1 = false;
	enablecmd2 = true;
	one_cmdtext = "";
	two_cmdtext = "OK";
	DialogEx.Show("Any_msg", true, nil, nil);
	DialogEx.Close(0);	
end
</Script>
