<Name>On Show</Name>
<Args/>
<Script>	--WAVES MOD LOCKER 3 DIALOG CONFIGURATION
function init_cfg()	
	Load_dados();
	ChangeDialogType();
	transparencyDiag = 255;
	while transparencyDiag &gt;= transparency_set do
		Slider.SetSliderPos("sld_trans", (Math.Round((transparencyDiag/2.55),0)));
		Input.SetText("txt_trans", (Math.Round((transparencyDiag/2.55),0)).."%");
		SetDialogTransparency(transparencyDiag);
		Application.Sleep(20);
		transparencyDiag = transparencyDiag - 2
	end
	
	--GTA
	
	function diagApl()
		diagcfgsa = ComboBox.GetSelected("cbb_gta");
		loadedgtadir = Input.GetText("txt_gtadir");
		INIFile.SetValue("Mod_Locker\\MOD_LOCKER.INI", "DIR", "gtadir", loadedgtadir);
		INIFile.SetValue("Mod_Locker\\MOD_LOCKER.INI", "INIT", "sainit", diagcfgsa);
		
		diagcfgsamp = ComboBox.GetSelected("cbb_samp");
		loadedsampdir = Input.GetText("txt_sampdir");
		INIFile.SetValue("Mod_Locker\\MOD_LOCKER.INI", "DIR", "sampdir", loadedsampdir);
		INIFile.SetValue("Mod_Locker\\MOD_LOCKER.INI", "INIT", "sampinit", diagcfgsamp);
		
		diagcfgmta = ComboBox.GetSelected("cbb_mta");
		loadedmtadir = Input.GetText("txt_mtadir");
		loadedmtaexedir = Input.GetText("txt_mtaexedir");
		INIFile.SetValue("Mod_Locker\\MOD_LOCKER.INI", "DIR", "mtadir", loadedmtadir);
		INIFile.SetValue("Mod_Locker\\MOD_LOCKER.INI", "DIR", "mtaexedir", loadedmtaexedir);
		INIFile.SetValue("Mod_Locker\\MOD_LOCKER.INI", "INIT", "mtainit", diagcfgmta);
		
		INIFile.SetValue("Mod_Locker\\MOD_LOCKER.INI", "GERAL", "transparency_set", transparencyDiag);
	end
	
	function gtaswitchON()
		Button.SetEnabled("cmd_vergta", true);
		ComboBox.SetEnabled("cbb_gta", true);
		exportdir = gtadir;
		diagplug();
		Label.SetText("lbl_gtaplug", nsetplug);
		Button.SetEnabled("cmd_getgtadir", true);
		Button.SetEnabled("cmd_getgtadir2", true);	
	end
	function gtaswitchOFF()
		Button.SetEnabled("cmd_vergta", false);
		ComboBox.SetEnabled("cbb_gta", false);
		Label.SetText("lbl_gtaplug", "CLEO: 0 | ASI: 0 | Total: 0");
		Button.SetEnabled("cmd_getgtadir", false);
		Button.SetEnabled("cmd_getgtadir2", false);
	end
	
	function diagcarregaSA()
		Input.SetText("txt_gtadir", gtadir);
		ComboBox.SetSelected("cbb_gta", sainit);
	end
	if gtadir ~= "" then
		gtaswitchON();
	else
		gtaswitchOFF();
	end
	diagcarregaSA();
	--end GTA
	
	--samp
	function sampswitchON()
		Button.SetEnabled("cmd_versamp", true);
		ComboBox.SetEnabled("cbb_samp", true);
		if gtadir == "" then
			Button.SetEnabled("cmd_getgtadir", false);
		else
			Button.SetEnabled("cmd_getgtadir", true);
		end
		exportdir = sampdir;
		diagplug();
		Label.SetText("lbl_sampplug", nsetplug);	
	end
	function sampswitchOFF()
		Button.SetEnabled("cmd_versamp", false);
		ComboBox.SetEnabled("cbb_samp", false);
		if gtadir == "" then
			Button.SetEnabled("cmd_getgtadir", false);
		else
			Button.SetEnabled("cmd_getgtadir", true);
		end
		Label.SetText("lbl_sampplug", "CLEO: 0 | ASI: 0 | Total: 0");
	end
	
	function diagcarregaSAMP()
		Input.SetText("txt_sampdir", sampdir);
		ComboBox.SetSelected("cbb_samp", sampinit);
	end
	if sampdir ~= "" then
		sampswitchON();
	else
		sampswitchOFF();
	end
	diagcarregaSAMP();
	--end samp
	
	--MTA
	function mtaswitchON()
		if mtadir ~= "" then
			Button.SetEnabled("cmd_vermta", true);
		else
			Button.SetEnabled("cmd_vermta", false);
		end
		ComboBox.SetEnabled("cbb_mta", true);
		exportdir = mtadir;
		if gtadir == "" then
			Button.SetEnabled("cmd_getgtadir2", false);
		else
			Button.SetEnabled("cmd_getgtadir2", true);
		end
		diagplug();
		Label.SetText("lbl_mtaplug", nsetplug);	
	end
	function mtaswitchOFF()
		if mtadir ~= "" then
			Button.SetEnabled("cmd_vermta", true);
		else
			Button.SetEnabled("cmd_vermta", false);
		end
		ComboBox.SetEnabled("cbb_mta", false);
		if gtadir == "" then
			Button.SetEnabled("cmd_getgtadir2", false);
		else
			Button.SetEnabled("cmd_getgtadir2", true);
		end
		Label.SetText("lbl_mtaplug", "CLEO: 0 | ASI: 0 | Total: 0");
	end
	
	function diagcarregaMTA()
		Input.SetText("txt_mtadir", mtadir);
		ComboBox.SetSelected("cbb_mta", mtainit);
	end
	
	if mtadir ~= "" and mtaexedir ~= "" then
		mtaswitchON();
	else
		mtaswitchOFF();
	end
	
	diagcarregaMTA();
	Button.SetEnabled("cmd_apli", false);
end
init_cfg();</Script>
