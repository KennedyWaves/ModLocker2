<Script>local tbSelected = ListBox.GetSelected("ltb_incomsa");
if tbSelected then
	local listmark = tbSelected[1]
	local nNumSelected = ListBox.GetSelectedCount("ltb_incomsa");	
	for i = nNumSelected, 1, -1 do
		ListBox.DeleteItem("ltb_incomsa", tbSelected[i]);
	end
	ListBox.SelectItem("ltb_incomsa", listmark);
	Button.SetEnabled("cmd_apli", true);	
else
	img_msg = 2;
	txt_msg = "Por favor, selecione um ou mais Plug-Ins!";
	enablecmd1 = false;
	enablecmd2 = true;
	one_cmdtext = "";
	two_cmdtext = "OK";
	DialogEx.Show("Any_msg", true, nil, nil);
end</Script>
