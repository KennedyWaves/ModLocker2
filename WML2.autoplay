			img_msg = 4;
			txt_msg = "O executável \"gta_sa.exe\" não foi encontrado!";
			enablecmd1 = false;
			enablecmd2 = true;
			one_cmdtext = "";
			two_cmdtext = "OK";
			DialogEx.Show("Any_msg", true, nil, nil);
			switchOFF();
		end
	end
end	</Script>
