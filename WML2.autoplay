<Script>exportdir = gtadir;
Application.SaveValue("Mod_Locker", "exportdir", gtadir);
listconcattira = "";
totalincomsalist = ListBox.GetCount("ltb_incomsa");
for x=1,totalincomsalist do
	nomelist = ListBox.GetItemText("ltb_incomsa", x);
	listconcattira = String.Concat(listconcattira, "\r\n"..nomelist.."");
end
lidopadcom = TextFile.ReadToTable("Mod_Locker\\SA_padcom.db");
totallido = Table.Count(lidopadcom);
if lidopadcom[4] ~= "" then
	Table.Insert(lidopadcom, 4, "");
end
concatlidocom = Table.Concat(lidopadcom, "\r\n", 1, TABLE_ALL);
listconcattira = String.Concat(listconcattira, "\r\n"..concatlidocom.."");
DialogEx.Show("add_plugin", true, nil, nil);
if addplugin ~= "cancelado" then
	for x,y in pairs (selec_table) do
		ListBox.AddItem("ltb_incomsa", selec_table[x], "");
	end
end
Button.SetEnabled("cmd_apli", true);
</Script>
