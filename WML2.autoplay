<Script>Button.SetEnabled("cmd_apli", true);
if (e_Key == 13) then
	sampdir = Input.GetText("txt_sampdir");
	if sampdir == "" then
		sampswitchOFF();
	else
		chkgta = File.Find(sampdir, "gta_sa.exe", false, false, nil, nil);
		if chkgta then
			sampswitchON();
		else
			img_msg = 4;
			txt_msg = "O executável \"gta_sa.exe\" não foi encontrado!";
			enablecmd1 = false;
			enablecmd2 = true;
			one_cmdtext = "";
			two_cmdtext = "OK";
			DialogEx.Show("Any_msg", true, nil, nil);
			sampswitchOFF();
		end
	end
end	</Script>
