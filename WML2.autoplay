function carregaSA()
	if sainit == 01 then
		RadioButton.SetChecked("rdb_offallsa", true);
	elseif sainit == 02 then
		RadioButton.SetChecked("rdb_onallsa", true);
	elseif sainit == 03 then
		RadioButton.SetChecked("rdb_askofflistsa", true);
	else
		RadioButton.SetChecked("rdb_askallsa", true);
	end
	Input.SetText("txt_gtadir", gtadir);
	--listcomsa
	lidocom = TextFile.ReadToTable("Mod_Locker\\SA_listcom.db");
	if (lidocom) then
		if lidocom[4] ~= "" then
			Table.Insert(lidocom, 4, "");
		end
		totallido = Table.Count(lidocom);
		numero = 5;
		if lidocom[4] ~= "" then
			Table.Insert(lidocom, 4, "");
		end
		if totallido &gt; 4 then
			for x = 5,totallido do
				ListBox.AddItem("ltb_comsa", lidocom[x], "");
			end
		end
	else
		TextFile.WriteFromString("Mod_Locker\\SA_listcom.db", "--[[================WAVES INFORMATICA================\r\n======DEFINICAO PERSONALIZADA DE PLUG-INS COMPATIVEIS==\r\n=====================GTA SAN ANDREAS=============]]--\r\n", false);
		lidocom = TextFile.ReadToTable("Mod_Locker\\SA_listcom.db");
	end
	--fim
	--listincomsa
	lidoincom = TextFile.ReadToTable("Mod_Locker\\SA_listincom.db");
	if (lidoincon) then
		if lidoincom[4] ~= "" then
			Table.Insert(lidoincom, 4, "");
		end
		totallido = Table.Count(lidoincom);
		numero = 5;
		if lidoincom[4] ~= "" then
			Table.Insert(lidoincom, 4, "");
		end
		if totallido &gt; 4 then
			for x = 5,totallido do
				ListBox.AddItem("ltb_incomsa", lidoincom[x], "");
			end
		end
	else
		TextFile.WriteFromString("Mod_Locker\\SA_listincom.db", "--[[================WAVES INFORMATICA================\r\n======DEFINICAO PERSONALIZADA DE PLUG-INS INCOMPATIVEIS==\r\n=====================GTA SAN ANDREAS=============]]--\r\n", false);
		lidoincom = TextFile.ReadToTable("Mod_Locker\\SA_listincom.db");
	end
	chkpad = File.Find("Mod_Locker", "SA_padcom.db", false, false, nil, nil);
	if chkpad == nil then
		TextFile.WriteFromString("Mod_Locker\\SA_padcom.db", "--[[================WAVES INFORMATICA================\r\n==========DEFINICAO PADRAO DE MODS COMPATIVEIS=======\r\n=====================GTA SAN ANDREAS=============]]--\r\n\r\nImVehFT.asi\r\nDSS.cs\r\nMemory512.cs\r\nBulletHole.asi\r\nStreamMemFix1.0.asi\r\nStreamMemFix2.2_test1.asi\r\nStreamMemFix2.2_test2.asi\r\nCLEO.asi\r\nradarrect.asi\r\ngasoline.cs\r\nCarspawn.cs\r\nFixWheelTurnBack.cs\r\nHooks.asi\r\n", false);
	end	
	--fim
	inittimesa = inittimesa + 1;
end

Load_dados();
ChangeWindowType();
transparency = transparency_set;
SetWindowTransparency(transparency);
exportdir = gtadir;
if inittimesa == 0 or (String.CompareNoCase(gtadir, lastgtadir)~= 0) then
	if gtadir ~= "" then
		switchON();
	end
	carregaSA();
end

Image.Load("cmd_gtasa", "Mod_Locker\\imagens\\gtahigh.png");
Image.Load("cmd_samp", "Mod_Locker\\imagens\\sampnormal.png");
Image.Load("img_main", "Mod_Locker\\imagens\\wml_normal.png");
Image.Load("cmd_mtasa", "Mod_Locker\\imagens\\mtasanormal.png");

if gtadir == "" then
	switchOFF();
end
loadnplugin();

